{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production"
  },
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9"
    }
  },
  "regions": ["iad1"]
}